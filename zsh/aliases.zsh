alias reload!='. ~/.zshrc'

alias cls='clear' # Good 'ol Clear Screen command

alias art="php artisan"
alias art:ide="php artisan ide-helper:generate && php artisan ide-helper:meta && php artisan ide-helper:models -WR"
alias art:db="php artisan migrate:fresh --seed"
alias php@7.2="/usr/local/opt/php@7.2/bin/php"
alias php@7.3="/usr/local/opt/php@7.3/bin/php"
